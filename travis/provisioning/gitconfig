[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[user]
	name = Scott
	email = scott@luminal.io
[alias]
	st = status
	br = branch
	co = checkout
	ci = commit
	unstage = reset HEAD --
	last = log -1 HEAD
	slog = log --oneline
[url "git@github.com:"]
	insteadOf = https://github.com/
[color]
	ui = true
